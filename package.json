    "eslint-config-standard": "^13.0.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.14.2",
    "eslint-plugin-standard": "^4.0.0",
    "node-sass": "^4.13.1",
    "nodemon": "^1.19.1",
    "sass-loader": "^7.1.0",
    "webpack": "^4.36.1",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/react"
